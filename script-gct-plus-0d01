cat temp-2 | awk '
BEGIN {
  FS = "\t";
  OFS = "";
  ORS = "";}
{
 if(NR < 4)
   print $0 "\n";
 else
 {
  print $1 "\t" $2 ;
  for(i = 3; i <= NF; i++)
  {  print "\t" ($i+0.01); 
  }
  print "\n";
 }
}
'
