testoster <- read.delim("testoinput.txt")
wilcox <- wilcox.test(testoster[,1], testoster[,2], paired=F, exact=F, conf.int = T)
wilcox$p.value

sink("temp-res.txt")

for(i in 1:dim(testoster)[2]) {
 for(j in 1:dim(testoster)[2]) {
