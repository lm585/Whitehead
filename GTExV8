 abline(v = seq(-100,100,0.2), lty = 5, lwd = 0.5, col = "gray")
 abline(h = 0)
 abline(v = 0)
title(main=paste("muscle: ", "r=",round(cor1$estimate, digits = 3), " p=", format(cor1$p.value, digits=3), " (Spearman)", sep =""))

