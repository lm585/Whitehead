#
ls *.STAR.log | while read ll
do
 l1=`cat "$ll" | grep "Average input read length" | awk '{print $6}' `
 l2=`cat "$ll" | grep "Average mapped length" | awk '{print $5}' `
 m1=`cat "$ll" | grep "Uniquely mapped reads %"   | awk '{print $6}' `
 echo "$ll	$l1	$l2	$m1"
done

