#chr6 31982057 32002681 (20625-bp) . + . gene_id "ENSG00000244731.10"; gene_type "protein_coding"; gene_name "C4A"; 


a=1

while [ "$a" -lt 20625 ]
do
 e=`expr "$a" + 149`
 echo ">c4a.$a-$e"
  ~/C++/reSeqPrintChrRegion.fq temp.c4a.dna.plus1kbp.fa out chr6 "$a" "$e" | awk 'NR == 2'
 a=`expr "$a" + 5000`
done
